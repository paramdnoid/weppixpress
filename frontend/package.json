{
  "name": "weppixpress-frontend",
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "clean": "rimraf dist node_modules/.vite",
    "optimize:images": "imagemin 'src/assets/images/**/*' --out-dir=src/assets/images/optimized",
    "generate:icons": "pwa-asset-generator src/assets/images/logo-dark.svg public --icon-only --favicon --mstile --opaque false",
    "deploy": "npm run build && npm run deploy:sync",
    "deploy:sync": "rsync -avz --delete dist/ root@km14307.keymachine.de:/home/users/weppi/www/dev.weppixpress.com/frontend/ && ssh root@km14307.keymachine.de 'chown -R weppi:weppi /home/users/weppi/www/dev.weppixpress.com/frontend && chmod -R 755 /home/users/weppi/www/dev.weppixpress.com/frontend'"
  },
  "dependencies": {
    "@iconify/vue": "^4.1.1",
    "@tabler/core": "^1.0.0-beta20",
    "@vueuse/core": "^10.7.2",
    "axios": "^1.6.5",
    "lodash-es": "^4.17.21",
    "pinia": "^2.1.7",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "vue-virtual-scroller": "^2.0.0-beta.8"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/compiler-sfc": "^3.4.15",
    "@vue/test-utils": "^2.4.4",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^10.4.0",
    "imagemin-cli": "^7.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
    "imagemin-svgo": "^11.0.0",
    "imagemin-webp": "^8.0.0",
    "lighthouse": "^11.5.0",
    "pwa-asset-generator": "^6.3.1",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "sass": "^1.70.0",
    "sass-embedded": "^1.80.6",
    "terser": "^5.27.0",
    "typescript": "^5.3.3",
    "unplugin-auto-import": "^0.17.3",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.12",
    "vite-bundle-visualizer": "^1.0.1",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-compression2": "^0.12.0",
    "vite-plugin-image-optimizer": "^1.1.7",
    "vite-plugin-pwa": "^0.17.5",
    "vitest": "^1.2.2",
    "vue-tsc": "^3.0.6",
    "vue-eslint-parser": "^10.2.0",
    "workbox-precaching": "^7.0.0",
    "workbox-routing": "^7.0.0",
    "workbox-strategies": "^7.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
